{
  "name": "@kscommon/easy-api",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/easy-api.umd.cjs",
  "module": "./dist/easy-api.js",
  "description": "Wrapper for Axios that allows to easily requests",
  "scripts": {
    "dev": "vite --config ./build/dev.js --open --host",
    "build": "vite build --config ./build/build.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/easy-api.js",
      "require": "./dist/easy-api.umd.cjs"
    }
  },
  "dependencies": {
    "axios": "^1.3.5",
    "qs": "^6.11.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xukaishan/easy-api.git"
  },
  "keywords": [
    "Axios",
    "request"
  ],
  "author": "littlekai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xukaishan/easy-api/issues"
  },
  "homepage": "https://github.com/xukaishan/easy-api#readme"
}
